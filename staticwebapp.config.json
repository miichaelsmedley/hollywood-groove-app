{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.js", "/*.css", "/*.png", "/*.ico", "/*.svg", "/*.webmanifest"]
  },
  "routes": [
    {
      "route": "/__/auth/*",
      "rewrite": "/index.html"
    }
  ],
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "mimeTypes": {
    ".webmanifest": "application/manifest+json"
  }
}
